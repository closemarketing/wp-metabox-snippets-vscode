{
	"MetaBox | Adds main function to load MetaBox": {
		"prefix": "metafun",
		"body": [
			"//* Adds Metabox function",
			"add_filter( 'rwmb_meta_boxes', 'cmk_register_meta_boxes_${1:cpt}' );",
			"function cmk_register_meta_boxes_${1:cpt}( \\$meta_boxes ) {",
			"\t\\$prefix = '${1:cpt}_';",
			"",
			"\t// 2nd meta box",
			"\t\\$meta_boxes[] = array(",
			"\t\t'id'\t\t => 'standard',",
			"\t\t'title'\t  => '${2:title-metabox}',",
			"\t\t'post_types' => array( '${3:post-type}' ),",
			"\t\t'context'\t=> '${4:orientation}', //side / normal",
			"\t\t'priority'   => 'high',",
			"\t\t'autosave'   => true,",
			"\t\t'fields'\t => array(",
			"",
			"",
			"\t\t) //fields",
			"\t);",
			"",
			"\treturn \\$meta_boxes;",
			"}"
		]
	},
	"MetaBox | Adds field select for posts": {
		"prefix": "metapost",
		"body": [
			"//* Get posts in array",
			"\\$args_query = array(",
			"\t'post_type' => '${1:post-type}',",
			"\t'posts_per_page' => -1,",
			"\t'orderby' => 'title', // menu_order, rand, date",
			"\t'order' => 'ASC',",
			");",
			"\\$posts_array = get_posts( \\$args_query );",
			"\\$posts_options = array();",
			"foreach( \\$posts_array as \\$post_single ):",
			"\t\\$posts_options[\\$post_single->ID] = \\$post_single->post_title;",
			"endforeach;",
			"",
			"// SELECT BOX post type ${1:post-type}",
			"array(",
			"\t'name'\t=> 'Selecciona ${2:cpt-name}',",
			"\t'id'\t  => \"{$prefix}${1:post-type}\",",
			"\t'type'\t=> 'select',",
			"\t'options'     => \\$posts_options,",
			"\t'multiple'    => false,",
			"\t'std'\t => '',",
			"\t'placeholder' => 'Selecciona ${2:cpt-name}',",
			"),"
		]
	},
	"MetaBox | Adds field for Gallery": {
		"prefix": "metagal",
		"body": [
			"// GALLERY IMAGES",
			"array(",
			"\t'name'\t\t\t => 'Galería',",
			"\t'id'\t\t\t   => \"{\\$prefix}${3:slug}\",",
			"\t'type'\t\t\t => 'image_advanced',",
			"\t'max_file_uploads' => 10,",
			"),",
			"",
			"//* in functions.php / single",
			"\\$images_meta = get_post_meta( get_the_ID(), '{\\$prefix}${3:slug}', false );",
			"",
			"\\$image_ids = '';",
			"",
			"if ( !empty( \\$images_meta ) ) {",
			"\tforeach ( \\$images_meta as \\$image ) {",
			"\t\t\\$image_ids .= \\$image.',';",
			"\t}",
			"\t\\$image_ids = rtrim(\\$image_ids, \",\");",
			"\techo do_shortcode( '[smart-grid captions=\"false\"][gallery ids=\"'.\\$image_ids.'\"][/smart-grid]', false );",
			"}   "
		]
	},
