	"MetaBox | Get Select Field of actual Forms in Gravity Forms": {
		"prefix": "metafor",
		"body": [
			"//* Gets the forms in array",
			"\\$forms = GFAPI::get_forms();",
			"\\$forms_options = array();",
			"foreach( \\$forms as \\$form ):",
			"\t\\$forms_options[\\$form['id']] = \\$form['id'].' - '.\\$form['title'];",
			"endforeach;",
			"",
			"",
			"// SELECT BOX Forms",
			"array(",
			"\t'name'\t\t=> 'Selecciona Formulario',",
			"\t'id'\t\t  => \"{\\$prefix}${1:slug}\",",
			"\t'type'\t\t=> 'select',",
			"\t'options'\t => \\$forms_options,",
			"\t'multiple'\t=> false,",
			"\t'std'\t\t => '',",
			"\t'placeholder' => 'Selecciona un formulario',",
			"),"
		]
	},
	"MetaBox | Shows Gallery in public page": {
		"prefix": "metsimg",
		"body": [
			"\\$images = rwmb_meta( '${1:meta_key}', 'size=${2:image_size}' ); ",
			"",
			"if ( !empty( \\$images ) ) {",
			"\tforeach ( \\$images as \\$image ) {",
			"\techo '<img src=\"'.\\$image['url'].'\" alt=\"'.\\$image['alt'].'\" width=\"'.\\$image['width'].'\" height=\"'.\\$image['height'].'\" /></a>';",
			"\t}",
			"}"
		]
	}
